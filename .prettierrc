{
  "singleQuote": false,
  "bracketSpacing": false,
  "arrowParens": "avoid",
  "importOrder": ["^@/(.*)$", "^[.]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "classProperties",
    "[\"decorators\", { \"decoratorsBeforeExport\": true }]"
  ]
}
